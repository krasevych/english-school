{
  "name": "school",
  "version": "0.0.1",
  "description": "English School",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"npm run webpack:watch\" \"npm run dev:start\"",
    "webpack:watch": "better-npm-run webpack:watch",
    "dev:start": "better-npm-run dev:start",
    "test": "karma start",
    "build": "rimraf dist && webpack -p --env.prod --display-modules --display-reasons --progress --profile --bail --color",
    "build2": "SET NODE_ENV=prodaction& rimraf dist && webpack --config ./config/prod.babel.js"
  },
  "betterScripts": {
    "webpack:watch": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "PORT": 3001
      }
    },
    "dev:start": {
      "command": "node ./src/server",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "global": "^4.3.1",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-form": "^0.9.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-immutablejs": "^0.0.8",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.14",
